{
  "name": "dwner",
  "productName": "dwner",
  "description": "Application for downloading music from YouTube with ease.",
  "version": "0.2.4",
  "private": false,
  "author": "Jabster28 <justynboyer@gmail.com>",
  "copyright": "Â© 2019, Jabster28.",
  "homepage": "http://example.com",
  "main": "app/background.js",
  "build": {
    "appId": "me.glitch.dwner",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Jabster28",
        "repo": "dwner"
      }
    ]
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "test": "yarn lint",
    "start": "node build/start.js",
    "lint": "node node_modules/eslint/bin/eslint.js . --ignore-path .gitignore",
    "package": "webpack --config=build/webpack.app.config.js --env=production && build -p never",
    "release": "webpack --config=build/webpack.app.config.js --env=production && build -p always",
    "rwin": "webpack --config=build/webpack.app.config.js --env=production && build --win -p always"
  },
  "dependencies": {
    "audio-metadata": "^0.3.0",
    "electron-dl": "^1.14.0",
    "electron-log": "^3.0.6",
    "electron-updater": "^4.0.6",
    "express": "^4.17.1",
    "fs-jetpack": "^2.1.0",
    "jquery": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-object-rest-spread": "7.0.0-beta.3",
    "chai": "4.2.0",
    "css-loader": "3.4.2",
    "electron": "6.1.7",
    "electron-builder": "20.44.4",
    "electron-mocha": "8.2.0",
    "eslint": "6.8.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "mocha": "6.2.2",
    "source-map-support": "0.5.16",
    "spectron": "8.0.0",
    "style-loader": "0.23.1",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-merge": "4.2.2",
    "webpack-node-externals": "1.7.2"
  }
}
