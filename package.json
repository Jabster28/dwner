{
  "name": "dwner",
  "productName": "dwner",
  "description": "Application for downloading music from YouTube with ease.",
  "version": "0.2.4",
  "private": false,
  "author": "Jabster28 <justynboyer@gmail.com>",
  "copyright": "Â© 2019, Jabster28.",
  "homepage": "http://example.com",
  "main": "app/background.js",
  "build": {
    "appId": "me.glitch.dwner",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Jabster28",
        "repo": "dwner"
      }
    ]
  },
  "scripts": {
    "test": "yarn lint && yarn e2e",
    "e2e": "mocha --timeout 50000 \"./test/*.test.js\"",
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "lint": "eslint . --ignore-path .gitignore",
    "start": "node build/start.js",
    "package": "webpack --config=build/webpack.app.config.js --env=production && build -p never",
    "release": "webpack --config=build/webpack.app.config.js --env=production && build -p always",
    "rwin": "webpack --config=build/webpack.app.config.js --env=production && build --win -p always",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "alltomp3": "^0.2.11",
    "audio-metadata": "^0.3.0",
    "electron-dl": "^1.14.0",
    "electron-log": "^3.0.6",
    "electron-updater": "^4.0.6",
    "eslint": "^6.0.1",
    "express": "^4.17.1",
    "fs-jetpack": "^2.1.0",
    "jquery": "^3.4.1",
    "snyk": "^1.192.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^1.0.0",
    "electron": "^2.0.8",
    "electron-builder": "^20.38.4",
    "electron-chromedriver": "^6.0.0-beta.3",
    "electron-mocha": "^6.0.4",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mocha": "^5.2.0",
    "source-map-support": "^0.5.6",
    "spectron": "^3.8.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2"
  },
  "snyk": true
}
